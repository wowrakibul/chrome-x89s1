{
    "manifest_version": 3,
    "name": "Chromalyze",
    "version": "10.0",
    "description": "A simple color picker extension for designers.",
    "permissions": ["clipboardWrite"],
    "host_permissions": ["<all_urls>"],
    "action":{
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
